export type IconType = keyof typeof icons;

interface Props {
	name: IconType;
	class?: string;
	width?: number | string;
	height?: number | string;
	color?: string;
}

const {
	name,
	class: className = "",
	width,
	height,
	color,
	...rest
} = Astro.props as Props;

const IconComponent = icons[name];
---
{IconComponent ? (
<IconComponent
	class={className}
	width={width}
	height={height}
	color={color}
	{...rest}
/>
	) : null}
