{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/resources/category/([^/]+)",
      "destination": "/resources/category/$1/1"
    },
    {
      "source": "/learn/categories/([^/]+)",
      "destination": "/learn/categories/$1/1"
    }
  ]
}
